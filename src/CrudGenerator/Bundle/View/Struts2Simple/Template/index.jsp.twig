{% import "macros.twig" as macro %}
{% block imports %}

{% endblock %}
<%@page contentType="text/html" pageEncoding="UTF-8"%>
<%@taglib prefix="s" uri="/struts-tags"%>
<!DOCTYPE html>
<html lang="en">
<head>
    <base href="{{ basePath }}">
    <%@include file="/WEB-INF/include/metas.jspf" %>
    <title>{% if block('title') is not empty %} {{ block('title') }} | {% endif %}{{ config.project.name }}</title>
    <%@include file="/WEB-INF/include/stylesheets.jspf" %>
</head>

<body>
<%@include file="/WEB-INF/template/header.jspf" %>
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <div class="container">
                {% block content %}
                    <div class="jumbotron">
                        <h1>{{ config.project.name }}</h1>

                        <p>Hola :)</p>
                    </div>
                {% endblock %}
            </div>
        </div>

    </div>
</div>

<%@include file="/WEB-INF/template/footer.jspf" %>
<%@include file="/WEB-INF/include/javascripts.jspf" %>

</body>
</html>
