
{
    "chmod": "0755",
    "directories": ["src","vendor"],
    "files": [
        "app/config/config.yml",
        "app/config/services.yml",
        "app/config/parameters.yml"
    ],
    "finder": [
        {
            "exclude": [
                "composer.json",
                "README.md",
                "README",
                "README.markdown",
                "CHANGELOG.md",
                ".gitignore",
                "phpunit.xml.dist",
                "package.xml.tpl",
                "Tester",
                "Tests",
                "Test",
                "doc",
                "test"
            ],
            "in": "vendor/*"
        }
    ],
    "git-commit": "git-commit",
    "git-version": "git-version",
    "main": "app/console",
    "output": "build/crud-generator.phar",
    "stub": true
}
